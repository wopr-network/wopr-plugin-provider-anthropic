{
  "author": "",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.39",
    "@anthropic-ai/claude-code": "^2.1.29",
    "winston": "^3.11.0"
  },
  "description": "Anthropic Claude provider plugin for WOPR",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "wopr",
    "wopr-plugin",
    "provider",
    "anthropic",
    "claude"
  ],
  "license": "MIT",
  "main": "index.js",
  "name": "@wopr-network/wopr-plugin-provider-anthropic",
  "peerDependencies": {
    "wopr": "^0.0.1"
  },
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
