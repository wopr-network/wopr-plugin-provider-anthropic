{
  "author": "",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.49",
    "@anthropic-ai/claude-code": "^2.1.49",
    "@wopr-network/plugin-types": "^0.2.0",
    "winston": "^3.11.0"
  },
  "description": "Anthropic Claude provider plugin for WOPR",
  "devDependencies": {
    "@biomejs/biome": "^2.4.3",
    "@types/node": "^25.2.3",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "wopr",
    "wopr-plugin",
    "provider",
    "anthropic",
    "claude"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "name": "@wopr-network/wopr-plugin-provider-anthropic",
  "peerDependencies": {
    "wopr": "^0.0.1"
  },
  "scripts": {
    "build": "tsc",
    "check": "biome check index.ts tests/ && tsc --noEmit",
    "format": "biome format --write index.ts tests/",
    "lint": "biome check index.ts tests/",
    "lint:fix": "biome check --fix src/",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "type": "module",
  "version": "2.3.0",
  "wopr": {
    "plugin": {
      "category": "provider",
      "description": "Anthropic Claude API via Agent SDK",
      "name": "provider-anthropic"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "release": {
    "extends": "@wopr-network/semantic-release-config"
  }
}
